{
  "name": "digitalent_api",
  "version": "1.0.0",
  "description": "An API service to interact with the frontend",
  "main": "server.js",
  "scripts": {
    "start": "node dist/server.js",
    "start:dev": "nodemon --exec babel-node ./src/server.js",
    "server": "babel-node ./src/server.js",
    "migrate": "sequelize --options-path ./.sequelize db:migrate",
    "migrate:undo": "sequelize --options-path ./.sequelize db:migrate:undo",
    "create:migration": "sequelize --options-path ./.sequelize migration:generate --name",
    "create:seeder": "sequelize --options-path ./.sequelize seed:generate --name",
    "create:model": "sequelize --options-path ./.sequelize seed:generate --name",
    "seed": "sequelize --options-path ./.sequelize db:seed --name",
    "migrate:undo:all": "sequelize --options-path ./.sequelize db:migrate:undo:all",
    "seed:all": "sequelize --options-path ./.sequelize db:seed:all"
  },
  "author": "Jermaine Wando",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "event-stream": "^4.0.1",
    "express": "^4.18.2",
    "joi": "^17.7.0",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "mssql": "^9.0.1",
    "nodemon": "^2.0.20",
    "qs": "^6.11.2",
    "sequelize": "^6.25.7",
    "sequelize-cli": "^6.5.2",
    "sequelize-transforms": "^2.0.0",
    "tedious": "^15.1.2",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.18.5",
    "@babel/node": "^7.14.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.17.10",
    "@babel/preset-env": "^7.5.5",
    "babel-jest": "^27.0.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "eslint": "^8.28.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0"
  }
}
